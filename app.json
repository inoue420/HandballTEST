{
  "expo": {
    "name": "Handball Rules",
    "slug": "my-quiz-project",
    "version": "1.4.6",
    "orientation": "portrait",
    "icon": "./assets/handball Rules_icon3.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-tracking-transparency"
    ],
    "splash": {
      "image": "./assets/handball Rules_icon3_and.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.inotake420.handballrules"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/handball Rules_icon3_and.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.inotake420.handballrules"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8d5c7d0d-8822-45ba-9742-93fbbefcfe30"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-4399954903316919~8218294300",
    "ios_app_id": "ca-app-pub-4399954903316919~2165399531",
    "delay_app_measurement_init": true,
    "user_tracking_usage_description": "許可することで、より関連性の高い広告を表示することができるようになります。"
  },
  "ios": {
    "infoPlist": {
      "NSUserTrackingUsageDescription": "このデバイスの情報を使用してパーソナライズされた広告を表示します。"
    }
  },
  
  "plugins": [
    [
      "expo-tracking-transparency",
      {
        "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
      }
    ]
  ]

}
